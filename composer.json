{
    "name": "teon/smart_autocomplete",
    "type": "roundcube-plugin",
    "description": "Self-learning autocomplete algorithm that optimizes order of suggested recipients by analysing your previously accepted autocomplete suggestions.",
    "homepage": "https://github.com/teonsystems/roundcube-plugin-smart-autocomplete/",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Bostjan Skufca",
            "email": "bostjan@teon.si",
            "role": "lead"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "roundcube/plugin-installer": ">=0.1.7"
    },
    "extra": {
        "roundcube": {
            "min-version": "1.1.4",
            "sql-dir": "SQL",
            "post-install-script": "echo 'POST-INSTALL SCRIPT'",
            "post-update-script": "echo 'POST-UPDATE SCRIPT'"
        }
    }
}
